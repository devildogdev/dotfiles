typeset -U PATH path
path=( "/usr/local/sbin" "/usr/local/bin" "/usr/bin" "/usr/bin/site_perl" "/usr/bin/vendor_perl" "/usr/bin/core_perl" "$HOME/.local/bin" "$path[@]")
export PATH
export LC_ALL="en_US.UTF-8"
export LESSHISTFILE=-
export EDITOR="nvim"
export TERMINAL="alacritty"
export COLORTERM="truecolor"
